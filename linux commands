check version - 
 cat /etc/os-release

rd /s  - remove a folder and its contents 

type <file name> opens the file in cmd
del <filename> deletes the file

echo <some content>  > filename.<extension>  creating a file and writing some content on it
<filename> typing the filename justs opens the file using default app

move <filename/foldername> <path to move the file to>
e.g sudo mv home/speed/preprocess_dir.zip home/state/TensorFlowTTS/data



###########################################################
adjust brightness
source 
https://vitux.com/control-screen-brightness-from-ubuntu-terminal/

xrandr | grep " connected" | cut -f1 -d " "
xrandr --output LVDS-1 --brightness 0.75                     # LVDS-1, monitor identity
xrandr --output HDMI-0 --brightness 0.5 
xrandr --output DP-0 --brightness 0.5


 scp .\julius_dl_front.jpg root@206.189.13.33:/var/images/Images/Documents

open vscode as admin
code . --no-sandbox --user-data-dir .

select editor
sudo select-editor

start service at reboot
crontab -e
@reboot  /home/user/startup.sh


