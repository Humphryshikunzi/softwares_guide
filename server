ssh <name>@ip
tmux attach  // get to the same work environment as another user, attach helps to leave the server processes when the internet goes down.
tmux detach 4 // detach from 4
cat <file name>  // list the contents of a file
curl api.ipify.org // show ip address

python3 -m pip install <modulename>


kill a port 
   kill -9 (lsof -t -i: <portnumber>)  e.g
   kill -9 (lsof -t -i:2000) 
